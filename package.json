{
  "name": "pixelsnake2.0",
  "version": "2.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "pixel-snake-2.0"
  ],
  "author": "Maruf Ovi",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.example.pixelsnake",
    "win": {
      "target": "nsis",
      "icon": "build/icon.png",
      "artifactName": "pixelsnakeSetup.exe"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "directories": {
      "buildResources": "build"
    }
  }
}
